{
   "builds": {
      "web": {
         "default": true
      },
      "native": {
         "packager": "cordova",
         "cordova": {
            "config": {
               "id": "com.domain.Contacts",
               "name": "Contacts",
               "platforms": "android"
            }
         }
      }
   },
   "name": "Contacts",
   "indexHtmlPath": "index.html",
   "framework": "touch",
   "url": null,
   "js": [
      {
         "path": "touch/sencha-touch.js",
         "x-bootstrap": true
      },
      {
         "path": "bootstrap.js",
         "x-bootstrap": true
      },
      {
         "path": "app.js",
         "bundle": true,
         "update": "delta"
      }
   ],
   "css": [
      {
         "update": "delta",
         "platform": [
            "phone",
            "tablet",
            "desktop"
         ],
         "theme": "Default",
         "path": "resources/css/sencha-touch.css"
      }
   ],
   "appCache": {
      "cache": [
         "index.html"
      ],
      "network": [
         "*"
      ],
      "fallback": []
   },
   "resources": [
      "resources/images",
      "resources/icons",
      "resources/startup"
   ],
   "ignore": [
      ".svn$"
   ],
   "archivePath": "archive",
   "requires": [],
   "cache": {
      "enable": true,
      "deltas": "deltas"
   },
   "id": "c65fdef9-ad15-435e-99b1-d2a8efbb46a0"
}